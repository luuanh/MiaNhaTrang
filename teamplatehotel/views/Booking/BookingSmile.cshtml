@using TeamplateHotel.Handler
@using ProjectLibrary.Database
@{
    ViewBag.Title = "Messages";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
    Menu menu = ViewBag.Menu ?? new Menu();
}

<section class="breadcrumb-area overlay-dark-2 bg-5">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="breadcrumb-text text-center">
                    <h2>@GetLanguage.Language(Request.Cookies["LanguageID"].Value, "BookNow")</h2>
                    <ul>
                        <li><a href="~/Views/Home/Index.cshtml">@GetLanguage.Language(Request.Cookies["LanguageID"].Value, "Home")</a></li>
                        <li>@GetLanguage.Language(Request.Cookies["LanguageID"].Value, "BookNow")</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</section>
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="des-article">
                <br />
                <iframe src="@ViewBag.Url" scrolling="no" width="100%" marginheight="20px" height="1200px" frameborder="0" id="frame-booking" style="position: relative;
    overflow: scroll;
    min-height: 1500px;
    height: auto;"></iframe>
                <div class="clear"></div>
            </div>
        </div>
    </div>
</div>
